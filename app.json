{
  "expo": {
    "name": "RemindMeX",
    "slug": "RemindMeX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/MainLogo.png",
    "scheme": "remindmex",

    "notification": {
      "icon": "./assets/icons/MainLogo.png",

      "color": "#ffffff"
    },

    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "cover",
      "image": "./assets/images/splash.png",
      "backgroundColor": "#ffffff"
    },

    "newArchEnabled": true,

    "web": {
      "output": "static",
      "favicon": "./assets/icons/MainLogo.png"
    },

    "extra": {
      "router": {
        "initialRouteName": "splash"
      },
      "eas": {
        "projectId": "9e68cb07-1153-4a0c-a1a3-cdce39896c8e"
      }
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icons/MainLogo.png",
        "backgroundImage": "./assets/icons/MainLogo.png",
        "monochromeImage": "./assets/icons/MainLogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.evin7.RemindMeX"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "imageWidth": 200,
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/splash.png"
          }
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/MainLogo.png",
          "color": "#ffffff",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
