{
  "expo": {
    "name": "RemindMeX",
    "slug": "RemindMeX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/MainLogo.png",
    "scheme": "remindmex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "output": "static",
      "favicon": "./assets/icons/MainLogo.png"
    },
    "extra": {
      "router": {
        "initialRouteName": "splash"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/MainLogo.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
